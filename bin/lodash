  _.each(['shimIsPlainObject', 'isPlainObject'], function(funcName) {
    source = source.replace(matchFunction(source, funcName), function(match) {
      return match.replace(/\s*\|\|\s*\(!support\.argsClass\b[\s\S]+?\)\)/, '');
    });
