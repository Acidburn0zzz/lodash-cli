{
  "name": "<%= name %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "homepage": "https://lodash.com/custom-builds",
  "license": "MIT",<%
  if (!_.isEmpty(keywords)) {
    print('\n  "keywords": ' + JSON.stringify(keywords).replace(/,/g, ', ') + ',');
  }%>
  "author": "John-David Dalton <john.david.dalton@gmail.com> (http://allyoucanleet.com/)",
  "contributors": [
    "John-David Dalton <john.david.dalton@gmail.com> (http://allyoucanleet.com/)",
    "Benjamin Tan <demoneaux@gmail.com> (https://d10.github.io/)",
    "Blaine Bublitz <blaine@iceddev.com> (http://www.iceddev.com/)",
    "Kit Cambridge <github@kitcambridge.be> (http://kitcambridge.be/)",
    "Mathias Bynens <mathias@qiwi.be> (https://mathiasbynens.be/)"
  ],
  "repository": "lodash/lodash-cli",
  "scripts": { "test": "echo \"See the repository CONTRIBUTING.md for testing instructions.\"" }<%
  if (!_.isEmpty(dependencies)) {
    print(',\n  "dependencies": ' + JSON.stringify(dependencies, null, 4).replace(/}$/, '  $&'));
  }%>
}
